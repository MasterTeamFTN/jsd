database=mysql

fields MyFields {
    hello varchar [unique]
    naziv varchar
}

entity Fakultet copy MyFields {
    naziv varchar [pk]
    godina_osnivanja number [unique notnull]
    grad varchar [notnull]
}

entity Predmet {
    naziv varchar [pk]
    broj_bodova number [notnull]
}

entity Student {
    id integer [pk]
    indeks integer
    *..* Fakultet
    *..* Predmet
}
