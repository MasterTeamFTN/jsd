database=mysql
type number
type varchar

entity Student {
    id number [pk]
    indeks varchar [unique notnull]
    ime varchar
    prezime varchar
    *..* Predmet
}

entity Predmet {
    naziv varchar [pk]
    opis varchar [notnull]
}

entity Fakultet {
    id varchar [pk]
    naziv varchar
    1..* Student prvazeljazaupis
    1..* Student drugazeljazaupis
}